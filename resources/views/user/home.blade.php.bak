                50% {
                    transform: translateY(-20px) translateX(0);
                }
                75% {
                    transform: translateY(-10px) translateX(-10px);
                }
                100% {
                    transform: translateY(0) translateX(0);
                }
            }
            
            .particle {
                position: absolute;
                background-color: white;
                border-radius: 50%;
                pointer-events: none;
            }
            
            /* Underline animation */
            .hover-underline-animation {
                position: relative;
            }
            
